{
  "target": {
    "protocol": "https",
    "ip": "example.com",
    "api_port": 443
  },
  "ssoLogin": {
    "enabled": true,
    "headers": {
      "Content-Type": "application/json",
      "loginname": {
        "myusername": "user"
      }
    }
  },
  "tls_validation": {
    "enabled": true,
    "verify": true,
    "caCert": "/path/to/caCert.pem",
    "clientCert": "/path/to/client-cert.pem",
    "clientKey": "/path/to/client-key.pem"
  }
}
