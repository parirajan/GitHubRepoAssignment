{
  "connection": {
    "protocol": "https",
    "host": "somehost",
    "port": 8001
  },
  "endpoints": {
    "session": {
      "postRequest": {
        "url": "/",
        "request_type": "POST"
      }
    },
    "api": {
      "getInstancesTable": {
        "url": "/",
        "request_type": "GET"
      }
    }
  },
  "headers": {
    "tokenname": {
      "myusername": "user"
    },
    "Content-Type": "application/json"
  },
  "uid": 100,
  "tls_enabled": true,  # Whether TLS is enabled
  "tls_verify": true,    # Whether to verify TLS with certs
  "tls_options": {
    "cert": "/path/to/client-cert.pem",   # Path to client cert
    "key": "/path/to/client-key.pem",     # Path to client key
    "ca_file": "/path/to/ca-cert.pem"     # Path to CA cert (optional)
  }
}
