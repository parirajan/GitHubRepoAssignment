{
    "target": {
        "ip": "api.example.com",
        "apiPort": "443"
    },
    "ssoLogin": {
        "enabled": true,
        "reqHeaders": {
            "Authorization": "Bearer some_token"
        }
    },
    "validateTls": {
        "enabled": true,
        "caCert": "/path/to/ca-cert.pem",
        "clientCert": "/path/to/client-cert.pem",
        "clientKey": "/path/to/client-key.pem"
    },
    "endpoints": {
        "loginUrl": "/login",
        "clusterHealthUrl": "/getClusterHealth",
        "fliStatusUrl": "/getFLIStatus"
    }
}
