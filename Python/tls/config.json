{
  "base_url": "https://your-api-url.com",
  "port": 443,
  "oidc_login": true,
  "tls_verify": true,
  "cert": {
    "cert_path": "/path/to/cert.pem",
    "key_path": "/path/to/key.pem"
  },
  "endpoints": {
    "tryssologin": "/api/login",
    "get_instances": "/api/getInstancesTable"
  },
  "auth": {
    "username": "user",
    "password": "pass"
  },
  "headers": {
    "Content-Type": "application/json",
    "x-fn-oidc-info": {
      "loginname": "user"
    }
  },
  "uid": 123  # Example UID
}
