{
  "connection": {
    "protocol": "https",
    "host": "somehost",
    "port": 8001
  },
  "endpoints": {
    "session": {
      "postRequest": {
        "url": "/postRequest",
        "request_type": "POST"
      }
    },
    "api": {
      "getInstancesTable": {
        "url": "/getInstancesTable",
        "request_type": "GET"
      }
    }
  },
  "headers": {
    "tokenname": {
      "myusername": "user"
    },
    "Content-Type": "application/json"
  },
  "login_type": "trySsoLogin",  // The login type data
  "uid": 100,
  "tls_verify": true,
  "tls_options": {
    "cert": "/path/to/client-cert.pem",
    "key": "/path/to/client-key.pem",
    "ca_file": "/path/to/ca-cert.pem"
  }
}
